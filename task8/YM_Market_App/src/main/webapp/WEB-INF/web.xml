    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/page</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Test1Servlet</servlet-name>
    <servlet-class>ServletsFolder.Test1Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Test1Servlet</servlet-name>
    <url-pattern>/test1</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Test2Servlet</servlet-name>
    <servlet-class>ServletsFolder.Test2Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Test2Servlet</servlet-name>
    <url-pattern>/test2</url-pattern>
  </servlet-mapping>
